{
  "manifest_version": 2,

  "name": "Yellow",
  "description": "This extension allows the user to enter a phrase, and jump to the point in the video that matches that phrase.",
  "version": "1.0",
  "web_accessible_resources": ["captions.jpg"],
  "permissions": [
    "webRequest",
    "activeTab",
    "tabs",
    "<all_urls>",
    "storage"
  ],
  "background": {
    "scripts": ["background.js"]
  },
  "content_scripts": [ 
    { 
        "js": [ "jquery-3.5.1.min.js", "extension.js"], 
        "matches": [ "*://www.youtube.com/*"] 
    }
  ]
}