{
  "manifest_version": 2,

  "name": "Yellow",
  "description": "This extension allows the user to enter a phrase, and jump to the point in the video that matches that phrase.",
  "version": "1.0",
  "permissions": [
    "webRequest",
    "activeTab",
    "tabs",
    "<all_urls>",
    "storage"
  ],
  "browser_action": {
    "default_icon": "icon.png",
    "default_title": "Yellow",
    "default_popup": "popup.html"
  },
  "background": {
    "scripts": ["background.js"]
  },
  "content_scripts": [ 
    { 
        "js": [ "jquery-3.5.1.min.js", "extension.js"], 
        "matches": [ "https://*/*"] 
    }
  ]
}